parse_git_branch() {
     git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/(\1)/'
}
export PS1="\u@\h \[\e[32m\]\w \[\e[91m\]\$(parse_git_branch)\[\e[00m\]$ "


alias pp="python3 -m http.server "
alias ..="cd ../"
alias allr='for x in $(find . -name "*.txt"); do echo -e "\e[92m#########################################################"; echo $x; echo -e "#########################################################\e[0m"; cat $x; done;'
